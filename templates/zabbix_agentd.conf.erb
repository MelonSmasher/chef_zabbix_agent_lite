# Zabbix Agent Configuration Generated by Chef for <%= node['fqdn'] %> do not edit!
# To change values edit the node attributes through Chef.

<% node['zabbix']['agent']['config'].each do |option, value| -%>
<% if value.kind_of?(Array) %>
<%= option %>={ <%= value.join(",") %> }
<% end %>
<% if value.kind_of?(String) %>
<%= option %>={ <%= value %> }
<% end %>
<% end -%>
